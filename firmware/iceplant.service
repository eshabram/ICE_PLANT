[Unit]
Description=ICE_PLANT remote data collector for Corometrics 250cx fetal monitoring machine
After=network-online.target
Wants=network-online.target

[Service]
Type=simple
User=pi
WorkingDirectory=/home/pi/ICE_PLANT
ExecStart=/usr/bin/python3 /home/pi/ICE_PLANT/ice_plant.py
Restart=always
RestartSec=2

[Install]
WantedBy=multi-user.target
